{
  "name": "petems-cockpit",
  "version": "0.1.0",
  "author": "Peter Souter",
  "summary": "Module for installing and managing Cockpit (http://cockpit-project.org/)",
  "license": "Apache-2.0",
  "source": "git://github.com/petems/petems-cockpit",
  "project_page": "https://github.com/petems/petems-cockpit",
  "issues_url": "https://github.com/petems/petems-cockpit/issues",
