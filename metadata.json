{
  "name": "petems-cockpit",
  "version": "1.0.1",
  "author": "Peter Souter",
  "summary": "Module for installing and managing Cockpit (http://cockpit-project.org/)",
  "license": "Apache-2.0",
  "source": "git://github.com/petems/petems-cockpit",
  "project_page": "https://github.com/petems/petems-cockpit",
  "issues_url": "https://github.com/petems/petems-cockpit/issues",
  "tags": [
    "cockpit"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": [
        "20",
        "21",
        "22"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.3.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.0.0 < 5.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.11.0 <5.0.0"
    },
    {
      "name": "puppetlabs-apt",
      "version_requirement": ">=2.3.0 <3.0.0"
    },
    {
      "name": "puppetlabs-inifile",
      "version_requirement": ">= 1.5.0 <2.0.0"
    }
  ]
}
